<h1>Edit photo</h1>
<%= form_with model: @photo do |form| %>
  <div>
    <%= form.label :image_url %>
    <%= form.text_field :image_url %>
  </div>
  <div>
    <%= form.label :description %>
    <%= form.text_field :description %>
  </div>
  <div>
    <%= form.label :date %>
    <%= form.text_field :date %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end%>
